{
	"name": "Get All the Tabs",
	"version": "1.1",
	"description": "prints all opened tabs to a file",
	"permissions": [
		"activeTab",
		"tabs",
		"storage"
	],
	"background": {
		"scripts": ["background.js"],
		"persistent": true
	},

	"icons": {
		"16": "images/get_started16.png",
		"32": "images/get_started32.png",
		"48": "images/get_started48.png",
		"128": "images/get_started128.png"
	},
	"manifest_version": 2
}
